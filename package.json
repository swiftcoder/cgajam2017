    "url": "https://github.com/swiftcoder/cgajam/issues"
  },
  "homepage": "https://github.com/swiftcoder/cgajam#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "p5": "^0.5.11"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.12",
    "concurrently": "^3.4.0",
    "watchify": "^3.9.0"
  }
}
